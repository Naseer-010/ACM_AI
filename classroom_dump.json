      },
      {
        "courseId": "791189548741",
        "id": "792701148858",
        "text": "The following students have not submitted assignment 2. Please submit by the end of the day.\nAP23110020149,\u00a0\nAP23110010090,\u00a0\nAP24110011556,\u00a0\nAP24110011317,\u00a0\nAP23110010210",
        "state": "PUBLISHED",
